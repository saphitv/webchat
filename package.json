{
  "name": "node-security",
  "version": "0.0.0",
  "notes": {
    "extra-cli-options": "--ssl true --ssl-key server/cert/key.pem --ssl-cert server/cert/cert.pem"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.json --ssl true --ssl-key cert/key.pem --ssl-cert cert/certificate.pem --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "auth_server": "nodemon auth_server/bin/www --watch 'auth_server/**/*.js'",
    "chat_server": "nodemon chat_server/app.js --watch 'chat_server/**.js'",
    "db_server": "nodemon db/index.js --watch 'db/index.js'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/common": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@ngrx/effects": "^15.3.0",
    "@ngrx/entity": "^15.3.0",
    "@ngrx/router-store": "^15.3.0",
    "@ngrx/store": "^15.3.0",
    "@ngrx/store-devtools": "^15.3.0",
    "argon2": "^0.30.3",
    "cookie-parser": "^1.4.6",
    "daisyui": "^2.46.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "ngx-socket-io": "^4.4.0",
    "password-validator": "^5.3.0",
    "rxjs": "~7.5.0",
    "socket.io": "^4.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@types/jasmine": "~4.0.0",
    "@types/lodash": "^4.14.192",
    "autoprefixer": "^10.4.13",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.5"
  }
}
